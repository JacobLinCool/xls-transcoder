{
	"private": true,
	"name": "xls-transcoder",
	"version": "0.0.0",
	"scripts": {
		"prepare": "husky",
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"format": "prettier --write . --ignore-path .gitignore",
		"build:image": "selflare compile && docker buildx bake --push --set '*.platform=linux/arm64,linux/amd64'"
	},
	"dependencies": {
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240222.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"selflare": "^1.0.0",
		"typescript": "^5.3.3",
		"wrangler": "^3.30.1"
	},
	"lint-staged": {
		"*.{ts,json,yml,yaml,md}": [
			"prettier --write"
		]
	},
	"packageManager": "pnpm@8.15.9"
}
